# Required parameters (You need to change these)

AZURE_OPENAI_API_KEY=XXX
AZURE_OPENAI_ENDPOINT=https://[[my-resource-name]].openai.azure.com/
AZURE_OPENAI_RESOURCE_NAME=[[my-resource-name]]

# Optional parameters (The defaults are fine)

AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-large
AZURE_OPENAI_API_VERSION=2025-03-01-preview

APP_NAME=parliament-mcp
AWS_REGION=aws-region
ENVIRONMENT=local
SENTRY_DSN=placeholder
DISABLE_AUTH_SIGNATURE_VERIFICATION=true

MCP_HOST=0.0.0.0
MCP_PORT=8080

# Qdrant Configuration
# Option 1: Local Qdrant
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Option 2: Qdrant Cloud (use instead of above)
# QDRANT_URL=https://your-cluster-url.qdrant.tech
# QDRANT_API_KEY=your-api-key-here
